---
import { RouteGuard } from "../components/routeguard"
import TableModal from "../components/table/TableModal"
import Layout from "../layouts/Layout.astro"
---

<Layout title="Trips">
	<RouteGuard client:load="react">
		<div class=" flex flex-col items-center justify-start relative w-full overflow-hidden">
			<div>
				<TableModal caseType={"flight"} client:load />
			</div>
		</div>
	</RouteGuard>
</Layout>
